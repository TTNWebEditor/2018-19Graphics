<style>

@import url('https://fonts.googleapis.com/css?family=Roboto,100,100italic,300,300italic,400,400italic,500,500italic,700,700italic,900,900italic,');
  	h1, h2, h3, h4, h5, h6, p {font-family: Roboto, sans-serif}

#received-dot {
  height: 25px;
  width: 25px;
  background-color: #178ab0;
  border-radius: 50%;
  display: inline-block;
}
#did-not-receive-dot {
  height: 25px;
  width: 25px;
  background-color: #88d6f0;
  border-radius: 50%;
  display: inline-block;
}
#did-not-apply-dot {
  height: 25px;
  width: 25px;
  background-color: #bbbbbb;
  border-radius: 50%;
  display: inline-block;
}

.label {
  	clear: both;
  	margin: 12px 0px 12px 0px;
}
  	
.Applied:hover #received-dot{
    background-color: #0e536b;
}


.Applied:hover #did-not-receive-dot{
    background-color: #10617c;
}


.Did-Not-Apply:hover #did-not-apply-dot {
  	background-color: #999999;
}

/*.Applied:hover .applied{
    background-color: #10617c;
}
*/

.tooltip {
	position: relative;
	display: inline;
}
	.tooltip .tooltiptext {
	    visibility: hidden;
	    width: 200px;
	    background-color: #e5e5e5;
	    color: black;
	    text-align: center;
	    border-radius: 6px;
	    padding: 5px 5px;
	    font-family: Roboto;
	    font-size: 1em;
	    
	    /* Position the tooltip */
	    position: absolute;
	    z-index: 1;
	    top: 100%;
	    left: 15px;
	}
	.tooltip:hover .tooltiptext {
	    visibility: visible;
	}



</style>
</head>
